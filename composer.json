{
  "name": "php-woox/webman-cache",
  "type": "library",
  "license": "MIT",
  "description": "Webman plugin php-woox/webman-cache",
  "require": {
    "php": ">=8.2",
    "illuminate/cache": ">=11.0",
    "illuminate/config": ">=11.0",
    "symfony/console": "^7.0"
  },
  "require-dev": {
    "illuminate/database": ">=11.0",
    "illuminate/events": ">=11.0",
    "illuminate/filesystem": ">=11.0",
    "illuminate/redis": ">=11.0",
    "phpunit/phpunit": "^11",
    "psr/cache": "^3.0",
    "psr/simple-cache": "^3.0",
    "symfony/cache": "^7.0",
    "workerman/webman-framework": "^1.5"
  },
  "autoload": {
    "psr-4": {
      "Woox\\WebmanCache\\": "src"
    },
    "files": [
      "src/helper.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Woox\\Webman\\Cache\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  },
  "scripts": {
    "test": "phpunit --testdox --no-interaction"
  }
}
